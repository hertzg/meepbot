{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "module": "commonjs",
    "types": ["node"],
    "emitDecoratorMetadata": true,
    "target": "es2019",
    "lib": ["es2019"],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": ["**/*.spec.ts"],
  "include": ["**/*.ts"]
}
